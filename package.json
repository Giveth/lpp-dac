{
  "name": "lpp-dac",
  "version": "0.0.3",
  "description": "LiquidPledging plugin contract for dacs to issue a custom MiniMe Token for donations.",
  "main": "lib/LPPDac.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "[ ! -d build ] && npm run sol-compile; mocha --harmony",
    "sol-compile": "solcpiler",
    "js-compile": "babel --source-maps -d lib/ js/",
    "compile": "npm run sol-compile; npm run js-compile",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giveth/lpp-dac.git"
  },
  "keywords": [
    "liquid",
    "pledging",
    "ethereum",
    "solidity",
    "dac"
  ],
  "author": "perissology",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Giveth/lpp-dac/issues"
  },
  "homepage": "https://github.com/Giveth/lpp-dac#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "eth-contract-class": "0.0.6",
    "giveth-common-contracts": "^0.4.0",
    "liquidpledging": "0.0.10",
    "minimetoken": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.3.0",
    "ethereumjs-testrpc": "git://github.com/perissology/testrpc.git#81216dbc",
    "lerna": "^2.2.0",
    "solcpiler": "0.0.5",
    "web3": "^1.0.0-beta.24"
  }
}
