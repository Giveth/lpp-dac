{
  "name": "lpp-dac",
  "version": "0.1.6",
  "description":
    "LiquidPledging plugin contract for dacs to issue a custom MiniMe Token for donations.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "[ ! -d build ] && npm run sol-compile; mocha --harmony",
    "sol-compile": "solcpiler --solc-version v0.4.18+commit.9cf6e910",
    "lint": "solium --dir contracts",
    "compile": "npm run sol-compile",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giveth/lpp-dacs.git"
  },
  "keywords": ["liquid", "pledging", "ethereum", "solidity", "dac"],
  "author": "perissology",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Giveth/lpp-dacs/issues"
  },
  "homepage": "https://github.com/Giveth/lpp-dacs#readme",
  "dependencies": {
    "@aragon/os": "3.0.4",
    "chai": "^4.1.2",
    "giveth-common-contracts": "0.4.0",
    "giveth-liquidpledging": "0.1.1",
    "minimetoken": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.3.0",
    "ganache-cli": "^6.0.1",
    "lerna": "^2.2.0",
    "solcpiler": "https://github.com/perissology/solcpiler.git#6393e66",
    "eth-contract-class": "0.0.8",
    "solium": "^1.0.9",
    "web3": "1.0.0-beta.31"
  }
}
